# AI Services Environment Variables

# Anthropic Claude API
CLAUDE_API_KEY=your_claude_api_key_here

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud Vision API
GOOGLE_CLOUD_VISION_KEY=your_google_vision_key_here

# PubMed API (optional)
PUBMED_API_KEY=your_pubmed_key_here

# OpenFDA API (no key required, but can be used for higher limits)
OPENFDA_API_KEY=

# RxNorm API (no key required)
RXNORM_API_KEY=

# Database connection (if needed for caching)
DATABASE_URL=sqlite:///ai_cache.db

# Logging level
LOG_LEVEL=INFO

# Cache settings
CACHE_TTL_SECONDS=3600
MAX_CACHE_SIZE_MB=100
